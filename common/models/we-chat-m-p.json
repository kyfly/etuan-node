{
  "name": "WeChatMP",
  "base": "PersistedModel",
  "strict": true,
  "idInjection": true,
  "properties": {
    "organizationUid": {
      "type": "object"
    },
    "mpOriginId": {
      "type": "string",
      "comments": "公众号原始id"
    },
    "interfaceUrl": {
      "type": "string"
    },
    "interfaceToken": {
      "type": "string"
    },
    "appId": {
      "type": "string"
    },
    "appSecret": {
      "type": "string"
    },
    "redirectUri": {
      "type": "string"
    },
    "updatedAt": {
      "type": "date"
    },
    "interfaceKey": {
      "type": "string",
      "comments": "用于AES加密的key"
    }
  },
  "validations": [],
  "relations": {
    "organization": {
      "type": "belongsTo",
      "model": "OrganizationUser",
      "foreignKey": "organizationUid"
    },
    "autoReplies": {
      "type": "embedsMany",
      "model": "WeChatMPAutoReply",
      "option": {
        "validate": true,
        "autoId": true
      }
    }
  },
  "acls": [],
  "methods": []
}
