{
  "name": "FormResult",
  "base": "BaseResult",
  "strict": true,
  "idInjection": true,
  "properties": {
    "messages": {
      "type": ["object"],
      "comments": "给用户的信息" 
    }
  },
  "validations": [],
  "relations": {
    "form": {
      "type": "belongsTo",
      "model": "Form",
      "foreignKey": "formId"
    },
    "answers": {
      "type": "embedsMany",
      "model": "FormResultAnswer",
      "option": {
        "validate": true,
        "autoId": true
      }
    }
  },
  "acls": [
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    }
  ],
  "methods": []
}
