{
  "name": "BaseItem",
  "base": "PersistedModel",
  "strict": true,
  "idInjection": false,
  "hidden": [
    "organizationUid"
  ],
  "properties": {
    "title": {
      "type": "string"
    },
    "logoUrl": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "adPicture": {
      "type": "string",
      "comments": "广告图片地址"
    },
    "adUrl": {
      "type": "string",
      "comments": "广告指向的地址"
    },
    "verifyRule": {
      "type": "string"
    },
    "updatedAt": {
      "type": "date"
    },
    "viewCount": {
      "type": "number",
      "default": 0
    },
    "university": {
      "type": "string"
    },
    "type": {
      "type": "string"
    },
    "school": {
      "type": "string"
    },
    "organizationName": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "qrCode": {
      "type": "hasOne",
      "model": "WeChatEtuanQR",
      "foreignKey": "itemId"
    },
    "organization": {
      "type": "belongsTo",
      "model": "OrganizationUser",
      "foreignKey": "organizationUid"
    }
  },
  "acls": [
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY",
      "property": "__get__organization"
    }
  ],
  "methods": {}
}
