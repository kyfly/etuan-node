{
  "name": "WeChatUser",
  "base": "User",
  "strict": true,
  "idInjection": true,
  "properties": {
    "openid": {
      "type": "string",
      "comments": "微信提供的uid"
    },
    "sex": {
      "type": "number",
      "comments": "0未知，1男，2女"
    },
    "province": {
      "type": "string",
      "comments": "省份"
    },
    "city": {
      "type": "string",
      "comments": "城市"
    },
    "country": {
      "type": "string",
      "comments": "国家"
    },
    "headImgUrl": {
      "type": "string",
      "comments": "头像url"
    },
    "privilege": {
      "type": "string",
      "comments": "权限"
    },
    "updatedAt": {
      "type": "date",
      "comments": "更新时间"
    },
    "university": {
      "type": "string",
      "comments": "所在大学"
    },
    "studentName": {
      "type": "string",
      "comments": "学生姓名"
    },
    "studentId": {
      "type": "number",
      "comments": "学号"
    },
    "verifiedDate": {
      "type": "date",
      "comments": "完成学号密码验证的时间"
    }
  },
  "validations": [],
  "relations": {
    "histories": {
      "type": "embedsMany",
      "model": "WeChatUserHistory",
      "option": {
        "validate": true,
        "autoId": true
      }
    },
    "formResults": {
      "type": "hasMany",
      "model": "FormResult",
      "foreignKey": "weChatUid"
    },
    "voteResults": {
      "type": "hasMany",
      "model": "VoteResult",
      "foreignKey": "weChatUid"
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__create__formResults"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__count__formResults"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__findById__formResults"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__get__formResults"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__destroyById__formResults"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__updateById__formResults"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__create__voteResults"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__count__voteResults"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__findById__voteResults"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__get__voteResults"
    }
  ],
  "methods": []
}
