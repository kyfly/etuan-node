{
  "name": "Form",
  "base": "PersistedModel",
  "strict": true,
  "idInjection": true,
  "properties": {
    "title": {
      "type": "string"
    },
    "startTime": {
      "type": "date"
    },
    "stopTime": {
      "type": "date"
    },
    "adPicture": {
      "type": "string",
      "comments": "广告图片地址"
    },
    "adUrl": {
      "type": "string",
      "comments": "广告指向的地址"
    },
    "verifyRule": {
      "type": "string"
    },
    "updatedAt": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "results": {
      "type": "hasMany",
      "model": "FormResult",
      "foreignKey": "formId"
    },
    "organization": {
      "type": "belongsTo",
      "model": "OrganizationUser",
      "foreignKey": "organizationUid"
    },
    "questions": {
      "type": "embedsMany",
      "model": "FormQuestion",
      "option": {
        "validate": true,
        "autoId": true
      }
    },
    "qrCode": {
      "type": "hasOne",
      "model": "WeChatEtuanQR",
      "foreignKey": "itemId"
    }
  },
  "acls": [],
  "methods": []
}
